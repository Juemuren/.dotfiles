{
    "$help": "https://aka.ms/terminal-documentation",
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions": 
    [
        {
            "command": "find",
            "id": "User.find"
        },
        {
            "command": 
            {
                "action": "copy",
                "singleLine": false
            },
            "id": "User.copy"
        },
        {
            "command": "paste",
            "id": "User.paste"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "id": "User.splitPane.auto"
        },
        {
            "command": 
            {
                "action": "scrollToMark",
                "direction": "previous"
            },
            "id": "User.preMark"
        },
        {
            "command": 
            {
                "action": "scrollToMark",
                "direction": "next"
            },
            "id": "User.nextMark"
        }
    ],
    "alwaysShowTabs": true,
    "centerOnLaunch": true,
    "copyFormatting": "none",
    "copyOnSelect": false,
    "defaultInputScope": "alphanumericHalfWidth",
    "defaultProfile": "{b6d28cf0-b245-59cb-a780-b728ed6c082e}",
    "keybindings": 
    [
        {
            "id": "User.find",
            "keys": "ctrl+f"
        },
        {
            "id": "User.paste",
            "keys": "ctrl+v"
        },
        {
            "id": "User.copy",
            "keys": "ctrl+c"
        },
        {
            "id": "User.splitPane.auto",
            "keys": "ctrl+shift+s"
        },
        {
            "id": "User.preMark",
            "keys": "ctrl+up"
        },
        {
            "id": "User.nextMark",
            "keys": "ctrl+down"
        }
    ],
    "newTabMenu": 
    [
        {
            "type": "remainingProfiles"
        },
        {
            "type": "separator"
        }
    ],
    "profiles": 
    {
        "defaults": 
        {
            "backgroundImageOpacity": 0.2,
            "bellStyle": "taskbar",
            "closeOnExit": "never",
            "colorScheme": "Dark+",
            "cursorShape": "filledBox",
            "font": 
            {
                "face": "Maple Mono NF CN",
                "size": 16
            },
            "showMarksOnScrollbar": true
        },
